{"id":7,"name":"minMax","agents":[{"archClass":"Jason","name":"bob","sourceCode":"listOfNumbers([1,2,3,4,5,6,7,8,9,0]).\n\n!start.\n!anyNumber(8).\n\n+!start: listOfNumbers(List) <- \n\t.min(List,Min); .max(List,Max);\n\t.print(\"The min is: \", Min ,\" and the max is: \", Max).\n\n+!anyNumber(N): listOfNumbers(List) & .max(List,N) <- .print(\"The number \", N, \" is the bigest in the list\").\n\n+!anyNumber(N): listOfNumbers(List) & .min(List,N) <- .print(\"The number \", N, \" is the smallest in the list\").\n\n-!anyNumber(N) <- .print(N, \" isn't nether biggest or smallest\").\n\n\n\n\n"}],"firmwares":[]}